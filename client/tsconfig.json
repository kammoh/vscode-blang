{
	"compilerOptions": {
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"module": "commonjs",
		"target": "es2019",
		"lib": ["ES2019"],
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
